                              <input type="text" maxlength="1" class="otpPass" id="otpPass2"/>
                              <input type="text" maxlength="1" class="otpPass" id="otpPass3"/>
                              <input type="text" maxlength="1" class="otpPass" id="otpPass4"/>
                              <input type="text" maxlength="1" class="otpPass" id="otpPass5"/>
                              <input type="text" maxlength="1" class="otpPass" id="otpPass6"/>
                          </div>
                      </div>
                      
                      <div class="procDiv" id="validateOTPPassBlock" style="display: none">
                          <button class="proceedBtn" id="proceedPassFunc">Proceed</button>
