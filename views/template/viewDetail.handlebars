  <div class="modal edituser" tabindex="-1" role="dialog" id="editViewDtl">
    <div class="modal-dialog" role="document">
      <div class="modal-content viewDtl_modal">
        <div class="modal-header editModal_head">
          <h5 class="edit_header ">Edit User data</h5>
          <button type="button btn" id="edit_close" class="edit_close">Close</button>
        </div>
        <div class="modal-body">
          <form name="editform" action="" id="editform">
            <div class="flexform">
              <div>
                <label for="">Full Name</label>
                <input type="text" class="caplet edit_input texOnly limitchar" id="edit_name" maxlength="40" />
              </div>
              <div>
                <label for="">Email Id</label>
                <input type="text" class="edit_input" id="edit_email" />
              </div>
              <div>
