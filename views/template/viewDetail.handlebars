                </select>
              </div>
              <div>
                <label for="">Designation</label>
                <select class="selectpicker edit_input edit_designation designation" title="Select designation"
                  id="edit_designation">
                </select>
              </div>
            </div>
            <button type="button" id="edit_submit" class="editsubmit">Submit</button>
          </form>
        </div>
      </div>
    </div>
  </div>
