<html lang="en">
  <head>
      <meta charset="UTF-8" />
      <meta http-equiv="X-UA-Compatible" content="IE=edge" />
      <meta name="viewport" content="width=device-width, initial-scale=1.0" />
      <meta name="csrf-token" content="{{csrfToken}}" />
      <link href="/assets/portalAsset/css/style.css" type="text/css" rel="stylesheet"/>
  {{!
  <link rel="stylesheet" href="/assets/portalAsset/css/bootstrap.min.css" /> }}
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" />
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fortawesome/fontawesome-free@5.15.4/css/fontawesome.min.css" integrity="sha384-jLKHWM3JRmfMU0A5x5AkjWkw/EYfGUAGagvnfryNV3F9VqM98XiIH7VBGVoxVSc7" crossorigin="anonymous">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <script type="text/javascript" src="./assets/portalAsset/js/jquery-1.10.2.js"></script>
  <script type="text/javascript" src="./assets/portalAsset/js/jquery.touchSwipe.js"></script>
  <script type="text/javascript" src="./assets/portalAsset/js/jquery.horizonScroll.js"></script>
</head>

<body>
  <div class="container-fluid no-pad">
    <div class="overall-Div">
      {{>sideBar}}
      <div class="content-div">
        {{>adminHeader}}
        <div class="adminMain">
          <h1 class="adHead">Admin</h1>
          <form id="AdminForm">
            <div class="row frRowDiv">
              <div class="col-3">
                <div class="AdInDiv">
                  <label>FullName</label>
                  <input type="text" class="AdIn form-control" id="fullName" placeholder="Fullname" />
                  <p class="ErrorMessage">Required</p>
                </div>
              </div>
              <div class="col-3">
                <div class="AdInDiv">
                  <label>Phone Number</label>
                  <input type="number" class="AdIn form-control" id="phoneNumber" placeholder="Phone Number"/>
                  <p class="ErrorMessage">Required</p>
                </div>
              </div>
              <div class="col-3">
                <div class="AdInDiv email-val">
                  <label>Email ID</label>
                  <input type="email" class="AdIn form-control" id="eMail" placeholder="Email ID"/>
                  <p class="ErrorMessage">Required</p>
                  <p class="emailError"> Enter correct email ID  </p>
                </div>
              </div>
              <div class="col-3">
                <div class="AdInDiv">
                  <label>Department</label>
                  <input type="text" class="AdIn form-control" id="department" placeholder="Department"/>
                  <p class="ErrorMessage">Required</p>

                </div>
              </div>
            </div>
            <div class="row frRowDiv">
              <div class="col-3">
                <div class="AdInDiv">
                  <label>Designation</label>
                  <input type="text" class="AdIn form-control" id="designation" placeholder="Designation" />
                  <p class="ErrorMessage">Required</p>

                </div>
              </div>
              <div class="col-3">
                <div class="AdInDiv">
                  <label>Date of Joining</label>
                  <input type="text" class="AdIn form-control" id="doj" placeholder="date of Joining" />
                  <p class="ErrorMessage">Required</p>

                </div>
              </div>
              <div class="col-3">
                <div class="AdInDiv">
                  <label>Work Location</label>
                  <input type="text" class="AdIn form-control" id="location" placeholder="Work Location" />
                  <p class="ErrorMessage">Required</p>

                </div>
              </div>
            </div>
            <input type="button" class="AdIn subBtn" value="Submit" id="saveDetails" />
          </form>
        </div>
      </div>
    </div>
  </div>
  <script src="js/admin.js"></script>
  <script src="/js/customToast.js" type="text/javascript"></script>
  <script src="/assets/js/plugins/bootstrap-notify.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js"></script>
  <script src="https://kit.fontawesome.com/e324a03398.js" crossorigin="anonymous"></script>
</body>

</html>